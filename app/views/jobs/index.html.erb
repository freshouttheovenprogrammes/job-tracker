All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul><%= link_to "#{job.title}", company_job_path(@company, job) %> at <%= job.company.name %></ul>
  <ul><%= link_to "Delete Job", company_job_path(@company, job), method: :delete %></ul>
<% end %>

<%= link_to "Add Job", new_company_job_path(@company) %>
